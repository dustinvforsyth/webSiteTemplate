{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-app.html",
    "src/shrine-detail.html",
    "src/shrine-featured-item.html",
    "src/shrine-item.html",
    "src/shrine-list.html",
    "src/shrine-simple-item.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/webcomponents-lite.js",
    "bower_components/webcomponentsjs/custom-elements-es5-adapter.js",
    "node_modules/babel-polyfill/dist/polyfill.min.js"
  ],
  "builds": [{
      "name": "es6",
      "js": {
        "minify": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "insertPrefetchLinks": true
    },
    {
      "name": "es5",
      "js": {
        "minify": false,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "insertPrefetchLinks": true
    }
  ]
}